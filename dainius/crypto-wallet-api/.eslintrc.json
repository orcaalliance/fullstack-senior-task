{
    "extends": "airbnb-base",
    "overrides": [
      {
        "files": ["**/*.js"],
        "rules": {
          "func-names": ["error", "never"],
          "import/no-extraneous-dependencies": ["error", "never"],
          "no-underscore-dangle": [1, { "allow": ["_id","_doc"] }]
        }
      }
    ]
}